{
  "name": "vector-retriever",
  "version": "1.0.0",
  "description": "General purpose vector database application with Qdrant and Ollama - featuring hybrid search, PII detection, and interactive visualization",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "embed": "node index.js embed",
    "search": "node index.js search",
    "hybrid": "node index.js hybrid",
    "demo": "node index.js demo",
    "examples": "node examples.js",
    "mixed": "node mixed_examples.js",
    "webui": "./start-webui.sh",
    "server": "node server.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "RUN_INTEGRATION_TESTS=true jest --testPathPattern=__tests__/integration --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && cd web-ui && npm run test && cd .. && npm run test:e2e"
  },
  "keywords": [
    "qdrant",
    "vector-database",
    "ollama",
    "embeddings",
    "semantic-search",
    "hybrid-search",
    "machine-learning",
    "AI"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@opendocsg/pdf2md": "^0.2.2",
    "@qdrant/js-client-rest": "^1.8.2",
    "axios": "^1.6.5",
    "canvas": "^3.2.0",
    "compromise": "^14.14.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "mammoth": "^1.11.0",
    "marked": "^17.0.1",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.4",
    "pdfjs-dist": "^5.4.449",
    "phone": "^3.1.69",
    "redis": "^5.10.0",
    "turndown": "^7.2.2",
    "umap-js": "^1.4.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
